package mypackage;  // Define the package name

public class MyClass {
    public void displayMessage() {
        System.out.println("Hello from MyClass inside mypackage!");
    }
}
import mypackage.MyClass;  // Import the class from the package

public class Main {
    public static void main(String[] args) {
        MyClass obj = new MyClass();  // Create object
        obj.displayMessage();         // Call method
    }
}
