#include<iostream>
using namespace std;
class raja{
    private:
    int value;
    public:
   
    raja(int v):  value (v){
        cout<<"constructor called value intialization:"<<value<<endl;

    }
    ~raja(){
        cout<<"destructor called value was:"<<value<<endl;

    }
    void display()const{
        cout<<"the value is:"<<value<<endl;

    }

};
int main(){
    raja s(34);
    s.display();
    return 0;
    
}
    
